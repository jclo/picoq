{
  "name": "picoq",
  "version": "0.1.1alpha6",
  "description": "A tiny Javascript library to interact with the DOM",
  "main": "index.js",
  "bin": {},
  "scripts": {
    "build": "gulp build",
    "watch": "gulp watch",
    "makedist": "gulp build && gulp makedist",
    "server": "node test/server.js",
    "servertravis": "node test/server.js &",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov mocha ./test/main.js",
    "check-coverage": "nyc check-coverage --statements 90 --branches 85 --functions 90 --lines 90",
    "display-coverage": "opener ./coverage/lcov-report/index.html",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "report": "nyc report",
    "doc": "vuepress dev docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/picoq.git"
  },
  "keywords": [
    "DOM",
    "jQuery"
  ],
  "author": {
    "name": "jclo",
    "email": "jclo@mobilabs.fr",
    "url": "http://www.mobilabs.fr/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/picoq/issues"
  },
  "homepage": "https://github.com/jclo/picoq",
  "dependencies": {
    "vuepress": "0.14.10"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "coveralls": "3.0.3",
    "cross-env": "5.2.0",
    "del": "4.1.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.0",
    "gulp": "4.0.0",
    "gulp-concat": "2.6.1",
    "gulp-footer": "2.0.2",
    "gulp-header": "2.0.7",
    "gulp-replace": "1.0.0",
    "gulp-uglify": "3.0.2",
    "husky": "1.3.1",
    "jsdom": "14.0.0",
    "mocha": "6.1.3",
    "nyc": "13.3.0",
    "opener": "1.5.1",
    "through2": "3.0.1",
    "xmlhttprequest": "1.8.0"
  },
  "private": false,
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run makedist && npm run test && npm run check-coverage && npm run report"
    }
  }
}
