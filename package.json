{
  "name": "picoq",
  "version": "0.0.10",
  "description": "A tiny Javascript library to interact with the DOM",
  "main": "index.js",
  "bin": {},
  "scripts": {
    "build": "gulp build",
    "watch": "gulp watch",
    "makedist": "gulp makejs && gulp makedist",
    "makeprivate": "gulp makeprivatepackage && sh tasks/compress.sh $npm_package_name $npm_package_version",
    "server": "node test/server.js",
    "servertravis": "node test/server.js &",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov _mocha ./test/main.js",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "display-coverage": "opener ./coverage/lcov-report/index.html",
    "report": "nyc report",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "doc": "cd docs && ../node_modules/.bin/gitbook --port 9999 serve",
    "precommit": "npm run makedist && npm run test && npm run check-coverage && npm run report"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/picoq.git"
  },
  "keywords": [],
  "author": {
    "name": "jclo",
    "email": "jclo@mobilabs.fr",
    "url": "http://www.mobilabs.fr/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/picoq/issues"
  },
  "homepage": "https://github.com/jclo/picoq",
  "dependencies": {},
  "devDependencies": {
    "chai": "4.1.2",
    "coveralls": "3.0.2",
    "cross-env": "5.2.0",
    "del": "3.0.0",
    "eslint": "5.4.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "gitbook-cli": "2.3.2",
    "gulp": "3.9.1",
    "gulp-concat": "2.6.1",
    "gulp-footer": "2.0.1",
    "gulp-header": "2.0.5",
    "gulp-replace": "1.0.0",
    "gulp-uglify": "3.0.1",
    "husky": "0.14.3",
    "jsdom": "12.0.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "opener": "1.5.0",
    "require-dir": "1.0.0",
    "run-sequence": "2.2.1",
    "through2": "2.0.3",
    "xmlhttprequest": "1.8.0"
  },
  "private": false
}
